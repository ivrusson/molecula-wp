{
  "name": "ivrusson/molecula-wp",
  "description": "React boilerplate for WordPress plugins",
  "require": {
    "php": ">=7.3",
    "slim/slim": "4.*",
    "slim/twig-view": "^3.2",
    "slim/psr7": "^1.3",
    "php-di/php-di": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "MoleculaWP\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
        "App\\Test\\": "tests/"
    }
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  }
}
